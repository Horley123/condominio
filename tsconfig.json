{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["app/*"]
    },
    "module": "commonjs",
    "target": "es5",
    "lib": ["dom", "es2015"],
    "jsx": "preserve",  
    "esModuleInterop": true,
    "skipLibCheck": true,
  
  },
  "include": [
    "**/*.ts",
    "**/*.js",
    "**/*.tsx",
    "**/d.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
